<?xml version="1.0" encoding="UTF-8"?>
<component name="confluence_pages_retrieval" type="synapse/template">

	<subComponents>
		<component name="getPages">
			<file>getPages.xml</file>
			<description>returns all the summaries in the space. Doesn't include
				pages which are in the Trash.
			</description>
		</component>
		<component name="getPage">
			<file>getPage.xml</file>
			<description>returns a single Page
			</description>
		</component>
	</subComponents>
</component>